program frrmMain_p;

uses
  Forms,
  frrmMain_u in 'frrmMain_u.pas' {frmMain},
  frmUser_u in 'frmUser_u.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmMain, frmMain);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
